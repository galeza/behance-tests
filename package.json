{
  "name": "behance-tests",
  "version": "1.0.0",
  "description": "Interview repo",
  "scripts": {
    "clean-reports": "rm -rf cypress/reports",
    "test": "npx cypress run",
    "merge-report": "npx mochawesome-merge cypress/reports/separate-reports/*.json  > cypress/reports/full_report.json",
    "generate-report": "npx mochawesome-report-generator --reportDir cypress/reports cypress/reports/full_report.json",
    "after:tests": "npm run merge-report; npm run generate-report",
    "cypress": "npm run clean-reports; npm run test; npm run after:tests"
  },
  "devDependencies": {
    "cypress": "^6.3.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.0.1",
    "mochawesome-report-generator": "^4.0.1"
  }
}
